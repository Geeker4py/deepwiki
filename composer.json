{
	"name": "deepdevelop/deepwiki",
	"description": "A pure markdown docs.",
	"license": "Apache License, Version 2.0",
	"authors": [
		{
			"name": "Yuan Chong",
			"email": "ychongsaytc@gmail.com",
			"homepage": "http://ychong.com"
		}
	],
	"minimum-stability": "dev",
	"config": {
		"vendor-dir": "deepwiki-vendor"
	},
	"require": {
		"erusev/parsedown-extra": "@stable",
		"bootstrap/bootstrap": "*",
		"jquery/jquery": "*",
		"php": ">=5.3.0"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "jquery/jquery",
				"version": "1.11.1",
				"dist": {
					"url": "https://code.jquery.com/jquery.min.js",
					"type": "file"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "bootstrap/bootstrap",
				"version": "3.2.0",
				"dist": {
					"url": "https://github.com/twbs/bootstrap/releases/download/v3.2.0/bootstrap-3.2.0-dist.zip",
					"type": "zip"
				}
			}
		}
	]
}
